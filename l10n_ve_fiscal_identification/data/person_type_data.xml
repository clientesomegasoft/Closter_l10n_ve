	</record>
	<record id="person_type_pnre" model="person.type">
		<field name="name">PNRE Persona Natural Residente</field>
		<field name="code">PNRE</field>
		<field name="is_company">False</field>
	</record>
	<record id="person_type_pnnr" model="person.type">
		<field name="name">PNNR Persona Natural No Residente</field>
		<field name="code">PNNR</field>
		<field name="is_company">False</field>
	</record>
</odoo>